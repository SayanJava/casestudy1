package com.java.demo;

class Addition{
	static int add(int x,int y) {
		return (x+y);
	}
	static int add(int x,int y,int z) {
		return(x+y+z);
	}
}

public class Poly {

	public static void main(String[] args) {
		System.out.println(Addition.add(1,2));
		System.out.println(Addition.add(5, 10,12));
		
	}
}
